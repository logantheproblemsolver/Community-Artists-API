    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate"
  },
  "keywords": [],
  "engines": {
    "node": "12.16.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.11",
    "helmet": "^3.21.3",
    "knex": "^0.20.11",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "uuid": "^7.0.2",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
